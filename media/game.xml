<?xml version="1.0" ?>
<gfx-xform version="1">
    <input file="game.png" type="png" tile-width="8" tile-height="8" width="240" height="224" />
    <output file="gfx_game.c">
        <tiles var-name="tiles_game"/>
        <maps pointers-size="8">
			<map var-name="map_screen" left="0" top="0" width="30" height="28"/>
			
			<map var-name="map_score_numbers" left="11" top="10" width="11" height="1"/>
			<map var-name="map_point_numbers" left="11" top="11" width="11" height="1"/>
			<map var-name="map_score_purple" left="4" top="17" width="1" height="1"/>
			
			<map var-name="map_bar_fills" left="11" top="21" width="9" height="1"/>
			
			<map var-name="map_pause" left="11" top="12" width="10" height="6"/>
			<map var-name="map_dialog_purple" left="12" top="13" width="1" height="1"/>
			<map var-name="map_pause_select" left="12" top="15" width="1" height="1"/>
			
			<map var-name="map_level_up" left="0" top="24" width="10" height="4"/>
			<map var-name="map_game_over" left="10" top="24" width="12" height="4"/>
			
			<map var-name="map_white" left="11" top="6" width="2" height="2"/>
			<map var-name="map_purple" left="13" top="6" width="2" height="2"/>
			<map var-name="map_green" left="15" top="6" width="2" height="2"/>
			<map var-name="map_orange" left="17" top="6" width="2" height="2"/>
			<map var-name="map_yellow" left="19" top="6" width="2" height="2"/>
			<map var-name="map_blue" left="21" top="6" width="2" height="2"/>
			<map var-name="map_red" left="23" top="6" width="2" height="2"/>
			<map var-name="map_clear" left="25" top="6" width="2" height="2"/>
			
			<map var-name="map_flash_white" left="11" top="8" width="2" height="2"/>
			<map var-name="map_flash_purple" left="13" top="8" width="2" height="2"/>
			<map var-name="map_flash_green" left="15" top="8" width="2" height="2"/>
			<map var-name="map_flash_orange" left="17" top="8" width="2" height="2"/>
			<map var-name="map_flash_yellow" left="19" top="8" width="2" height="2"/>
			<map var-name="map_flash_blue" left="21" top="8" width="2" height="2"/>
			<map var-name="map_flash_red" left="23" top="8" width="2" height="2"/>
		</maps>	
    </output>
</gfx-xform>